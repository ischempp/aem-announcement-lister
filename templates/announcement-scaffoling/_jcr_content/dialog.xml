<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Dialog"
    activeTab="0"
    xtype="tabpanel">
    <items jcr:primaryType="cq:WidgetCollection">
        <tab1
            jcr:primaryType="cq:Panel"
            title="Announcement">
            <items jcr:primaryType="cq:WidgetCollection">
                <title
                    jcr:primaryType="cq:Widget"
                    allowBlank="{Boolean}false"
                    fieldLabel="Title"
                    name="./jcr:content/jcr:title"/>
                <linkTo
                    jcr:primaryType="cq:Widget"
                    fieldDescription="If present, title links to this location in Announcement Lister component"
                    fieldLabel="Link announcement to"
                    name="./jcr:content/linkURL"
                    rootPath="/content/centernet/en"
                    xtype="pathfield"/>
                <openInNewWindow
                   jcr:primaryType="cq:Widget"
                   fieldLabel="Open in new window"
                   name="./jcr:content/isTargetBlank"
                   fieldDescription="Check to have this link open in a new browser tab or window"
                   defaultValue="{Boolean}false"
                   xtype="selection"
                   type="checkbox"/>
                <announcement
                    jcr:primaryType="cq:Widget"
                    fieldLabel="Announcement"
                    height="150"
                    name="./jcr:content/textimage/text"
                    xtype="richtext">
                    <rtePlugins jcr:primaryType="nt:unstructured">
                        <table
                            jcr:primaryType="nt:unstructured"
                            features="*"/>
                        <format
                            jcr:primaryType="nt:unstructured"
                            features="*"/>
                        <lists
                            jcr:primaryType="nt:unstructured"
                            features="*"/>
                        <justify
                            jcr:primaryType="nt:unstructured"
                            features="*"/>
                        <edit
                            jcr:primaryType="nt:unstructured"
                            features="[paste-wordhtml]"/>
                        <findreplace
                            jcr:primaryType="nt:unstructured"
                            features="*"/>
                        <paraformat
                            jcr:primaryType="nt:unstructured"
                            features="*"/>
                        <subsuperscript
                            jcr:primaryType="nt:unstructured"
                            features="*"/>
                        <misctools
                            jcr:primaryType="nt:unstructured"
                            features="[specialchars]"/>
                        <links
                            jcr:primaryType="nt:unstructured"
                            features="*"/>
                        <spellcheck
                            jcr:primaryType="nt:unstructured"
                            features="*"
                            invalidStyle="background-color: #ffdddd;"/>
                        <undo
                            jcr:primaryType="nt:unstructured"
                            features="*"/>
                        <image
                            jcr:primaryType="nt:unstructured"
                            features="*"/>
                    </rtePlugins>
                 <specialCharsConfig jcr:primaryType="nt:unstructured">
      <chars jcr:primaryType="nt:unstructured">
       <copyright
        jcr:primaryType="nt:unstructured"
        entity="&amp;copy;"/>
       <reg
        jcr:primaryType="nt:unstructured"
        entity="&amp;reg;"/>
       <trademark
        jcr:primaryType="nt:unstructured"
        entity="&amp;trade;"/>
       <emdash
        jcr:primaryType="nt:unstructured"
        entity="&amp;mdash;"/>
       <ellipsis
        jcr:primaryType="nt:unstructured"
        entity="&amp;hellip;"/>
       <Aacute
        jcr:primaryType="nt:unstructured"
        entity="&amp;Aacute;"/>
       <aacute
        jcr:primaryType="nt:unstructured"
        entity="&amp;aacute;"/>
       <Aring
        jcr:primaryType="nt:unstructured"
        entity="&amp;Aring;"/>
       <Eacute
        jcr:primaryType="nt:unstructured"
        entity="&amp;Eacute;"/>
       <eacute
        jcr:primaryType="nt:unstructured"
        entity="&amp;eacute;"/>
       <Iacute
        jcr:primaryType="nt:unstructured"
        entity="&amp;Iacute;"/>
       <iacute
        jcr:primaryType="nt:unstructured"
        entity="&amp;iacute;"/>
       <Oacute
        jcr:primaryType="nt:unstructured"
        entity="&amp;Oacute;"/>
       <oacute
        jcr:primaryType="nt:unstructured"
        entity="&amp;oacute;"/>
       <Ouml
        jcr:primaryType="nt:unstructured"
        entity="&amp;Ouml;"/>
       <ouml
        jcr:primaryType="nt:unstructured"
        entity="&amp;ouml;"/>
       <Uacute
        jcr:primaryType="nt:unstructured"
        entity="&amp;Uacute;"/>
       <uacute
        jcr:primaryType="nt:unstructured"
        entity="&amp;uacute;"/>
       <Uuml
        jcr:primaryType="nt:unstructured"
        entity="&amp;Uuml;"/>
       <uuml
        jcr:primaryType="nt:unstructured"
        entity="&amp;uuml;"/>
       <Ccedil
        jcr:primaryType="nt:unstructured"
        entity="&amp;Ccedil;"/>
       <ccedil
        jcr:primaryType="nt:unstructured"
        entity="&amp;ccedil;"/>
       <Ntilde
        jcr:primaryType="nt:unstructured"
        entity="&amp;Ntilde;"/>
       <ntilde
        jcr:primaryType="nt:unstructured"
        entity="&amp;ntilde;"/>
       <greekchars
        jcr:primaryType="nt:unstructured"
        rangeEnd="{Long}969"
        rangeStart="{Long}945"/>
       <greekcharscaps
        jcr:primaryType="nt:unstructured"
        rangeEnd="{Long}937"
        rangeStart="{Long}915"/>
      </chars>
     </specialCharsConfig>
                </announcement>
                <richFlag
                    jcr:primaryType="cq:Widget"
                    ignoreData="{Boolean}true"
                    name="./jcr:content/textimage/textIsRich"
                    value="true"
                    xtype="hidden"/>
                <resourceType
                    jcr:primaryType="cq:Widget"
                    ignoreData="{Boolean}true"
                    name="./jcr:content/textimage/sling:resourceType"
                    value="foundation/components/textimage"
                    xtype="hidden"/>
            </items>
        </tab1>
        <tab3
            jcr:primaryType="cq:Panel"
            title="On/Off Time">
            <items jcr:primaryType="cq:WidgetCollection">
                <ontime
                    jcr:primaryType="cq:Widget"
                    allowBlank="{Boolean}false"
                    ctCls="cq-propsdialog-ontime"
                    fieldLabel="On Time"
                    name="./jcr:content/onTime"
                    xtype="datetime">
					<listeners
						jcr:primaryType="nt:unstructured"
						loadcontent="function() { if(jstz.determine().name() != &quot;America/Los_Angeles&quot;) { alert('We’ve detected that you are in a different time zone than Seattle. The time and date fields on this page (on time, off time, start/stop date &amp; times) reflect your current local time zone, not Seattle’s.'); }}"/>						
				</ontime>
                <offtime
                    jcr:primaryType="cq:Widget"
                    allowBlank="{Boolean}false"
                    ctCls="cq-propsdialog-offtime"
                    fieldLabel="Off Time"
                    name="./jcr:content/offTime"
                    xtype="datetime"/>
                <onTimeHint
                    jcr:primaryType="cq:Widget"
                    ignoreData="true"
                    name="./jcr:content/onTime@TypeHint"
                    value="Date"
                    xtype="hidden"/>
                <offTimeHint
                    jcr:primaryType="cq:Widget"
                    ignoreData="true"
                    name="./jcr:content/offTime@TypeHint"
                    value="Date"
                    xtype="hidden"/>
            </items>
        </tab3>
    </items>
</jcr:root>
